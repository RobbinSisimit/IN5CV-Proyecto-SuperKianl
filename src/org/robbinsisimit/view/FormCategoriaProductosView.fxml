      <TextField fx:id="tfCategoriaProductoId" editable="false" layoutX="40.0" layoutY="160.0" prefHeight="35.0" prefWidth="300.0" promptText="idCategoriaProducto" />
      <TextField fx:id="tfNombreCategoria" layoutX="40.0" layoutY="269.0" prefHeight="35.0" prefWidth="300.0" promptText="NombreCategoriaProducto" />
      <TextField fx:id="tfDescripcionCategoria" layoutX="40.0" layoutY="371.0" prefHeight="35.0" prefWidth="300.0" promptText="DescripcionCategoria" />
      <Button fx:id="btnGuardar" layoutX="65.0" layoutY="465.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="45.0" prefWidth="81.0" text="Guardar" />
      <Button fx:id="btnCancelar" layoutX="190.0" layoutY="465.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="45.0" prefWidth="81.0" text="Cancelar" />
