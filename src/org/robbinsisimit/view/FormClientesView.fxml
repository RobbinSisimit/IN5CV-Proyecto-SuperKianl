      <TextField fx:id="tfClienteId" editable="false" layoutX="60.0" layoutY="117.0" prefHeight="42.0" prefWidth="303.0" promptText="Id" />
      <TextField fx:id="tfNombre" layoutX="55.0" layoutY="194.0" prefHeight="42.0" prefWidth="303.0" promptText="nombre" />
      <TextField fx:id="tfApellido" layoutX="55.0" layoutY="286.0" prefHeight="42.0" prefWidth="303.0" promptText="apellido" />
      <TextField fx:id="tfTelefono" layoutX="60.0" layoutY="367.0" prefHeight="42.0" prefWidth="303.0" promptText="telefono" />
      <TextField fx:id="tfDireccion" layoutX="55.0" layoutY="449.0" prefHeight="42.0" prefWidth="327.0" promptText="direccion" />
