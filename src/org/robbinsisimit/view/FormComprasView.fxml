      <TextField fx:id="tfTotalCompra" layoutX="65.0" layoutY="326.0" prefHeight="35.0" prefWidth="250.0" promptText="Total" />
      <Button fx:id="btnGuardar" layoutX="65.0" layoutY="424.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="70.0" text="Guardar" />
      <Button fx:id="btnCancelar" layoutX="190.0" layoutY="424.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="70.0" text="Cancelar" />
