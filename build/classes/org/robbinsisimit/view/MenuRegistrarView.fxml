      <Button fx:id="btnRegistrar" layoutX="55.0" layoutY="605.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="53.0" prefWidth="113.0" text="Registrar" />
      <Button fx:id="btnCrear" layoutX="216.0" layoutY="605.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="53.0" prefWidth="113.0" text="Crear Usuario" />
