      <ImageView fitHeight="650.0" fitWidth="450.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/FondoLogin.png" />
         </image>
      </ImageView>
      <PasswordField fx:id="tfPassword" layoutX="89.0" layoutY="423.0" prefHeight="41.0" prefWidth="306.0" promptText="tfPassword" />
      <TextField fx:id="tfUser" layoutX="89.0" layoutY="317.0" prefHeight="41.0" prefWidth="306.0" promptText="tfUser" />
      <Button fx:id="btnIniciar" layoutX="177.0" layoutY="478.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="57.0" prefWidth="130.0" text="Iniciar" />
      <Button fx:id="btnRegistrar" disable="true" layoutX="177.0" layoutY="543.0" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="57.0" prefWidth="130.0" text="Regristrar Usuario" wrapText="true" />
   </children>
