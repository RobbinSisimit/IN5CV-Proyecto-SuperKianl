<AnchorPane id="AnchorPane" prefHeight="650.0" prefWidth="450.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="org.robbinsisimit.controller.MenuLoginController">
